<?xml version="1.0" encoding="UTF-8"?>
<iw:iwidget xmlns:iw="http://www.ibm.com/xmlns/prod/iWidget" 
			id="ViewLimit"
			allowInstanceContent="true"
			supportedModes="view edit config help"
			supportedWindowStates="maximize minimize"
			iScope="com.cs.widgets.Summary" 
			lang="">
	<iw:resource globalid="com.cs.resources.SummaryJs" src="Summary/summary.js"/>
	<iw:resource globalid="com.cs.resources.SummaryCss" src="Summary/summary.css"/>
	<iw:itemSet id="attributes">
		<iw:item id="listCount" alias="listCount" lang=""
				 readOnly="false" value="5"/>
	</iw:itemSet>
	<iw:itemSet id="idescriptor">
		<iw:item id="title" alias="title" lang=""
				 readOnly="false" value="Summary">
		</iw:item>
	</iw:itemSet>
	<iw:itemSet id="labels">
		<iw:item id="norec" alias="norec" lang=""
				 readOnly="false" value="No Record">
		</iw:item>
	</iw:itemSet>
	<iw:itemSet id="view.labels">
		<iw:item id="total" alias="total" lang=""
				 readOnly="true" value="Total records">
		</iw:item>
		<iw:item id="more" alias="more" lang=""
				 readOnly="true" value="Open more">
		</iw:item>
		<iw:item id="norec" alias="more" lang=""
				 readOnly="true" value="No record">
		</iw:item>
	</iw:itemSet>
	<iw:itemSet id="edit.labels">
		<iw:item id="summaryType" alias="summaryType" lang=""
				 readOnly="true" value="Function list">
		</iw:item>
		<iw:item id="selector" alias="selector" lang=""
				 readOnly="true" value="Record count">
		</iw:item>
	</iw:itemSet>
	<iw:content mode="view"><![CDATA[
		<div class="com-cs-widget-Summary"></div>
	]]></iw:content>
	<iw:content mode="edit"><![CDATA[
		<div class="com-cs-widget-Summary">
			<div class="edit-content">
				<div class="content-row">
					<div class="selector-label-s label">Summary type</div>
					<div class="summary-type"></div>
				</div>		
				<div class="content-row">
					<div class="selector-label-r label">Records count</div>
					<div class="records-count"></div>
				</div>
				
			</div>
		</div>
	]]></iw:content>
	<iw:content mode="help"><![CDATA[
		<div class="com-cs-widget-Summary">
			Summary list.
		</div>
	]]></iw:content>
</iw:iwidget>