<?xml version="1.0" encoding="UTF-8"?>
<iw:iwidget xmlns:iw="http://www.ibm.com/xmlns/prod/iWidget" 
			id="Framework"
			lang="">
	<iw:itemSet id="idescriptor">
		<iw:item id="service" alias="service" lang=""
				readOnly="true" value="com.cs.iwidget.framework.WidgetServiceImpl" />
	</iw:itemSet>
	<iw:itemSet id="attributes">
		<iw:item id="def_tab" alias="tab" lang=""
				readOnly="false" value="Home" />
		<iw:item id="def_layout" alias="layout" lang=""
				readOnly="false" value="layout" />
		<iw:item id="def_wrapper" alias="wrapper" lang=""
				readOnly="false" value="wrapper" />
		<iw:item id="theme" alias="theme" lang=""
				readOnly="false" value="default" />
		<iw:item id="tab.max" alias="max" lang=""
				readOnly="true" value="5" />
		<iw:item id="err.show.time" alias="time" lang=""
				readOnly="true" value="5000" />
	</iw:itemSet>
	<iw:itemSet id="page.common">
		<iw:item id="setting.loading" alias="" lang=""
				 readOnly="true" value="Loading...">
		</iw:item>
		<iw:item id="close" alias="" lang=""
				 readOnly="true" value="Close">
		</iw:item>
	</iw:itemSet>
	<iw:itemSet id="widget.tabs.labels">
		<iw:item id="tab.more" alias="more" lang=""
				 readOnly="true" value="More">
		</iw:item>
		<iw:item id="tab.create" alias="create" lang=""
				 readOnly="true" value="Add new tab">
		</iw:item>
		<iw:item id="tab.new" alias="new" lang=""
				 readOnly="true" value="New tab ">
		</iw:item>
		<iw:item id="tab.close" alias="close" lang=""
				 readOnly="true" value="Close">
		</iw:item>
	</iw:itemSet>
	<iw:itemSet id="widget.personalize.labels">
		<iw:item id="service.group.base" alias="base" lang=""
				 readOnly="true" value="Basic Services">
		</iw:item>
		<iw:item id="msg.notsaved" alias="msg" lang=""
				 readOnly="true" value="Modifications not saved.">
		</iw:item>
		<iw:item id="msg.load" alias="load" lang=""
				 readOnly="true" value="Saving...">
		</iw:item>
		<iw:item id="msg.done" alias="done" lang=""
				 readOnly="true" value="All changes saved">
		</iw:item>
		<iw:item id="msg.act.save" alias="act" lang=""
				 readOnly="true" value="Save">
		</iw:item>
		<iw:item id="msg.act.discard" alias="discard" lang=""
				 readOnly="true" value="Discard">
		</iw:item>
		<iw:item id="btn.save" alias="save" lang=""
				 readOnly="true" value="Save Changes">
		</iw:item>
		<iw:item id="btn.done" alias="back" lang=""
				 readOnly="true" value="Done">
		</iw:item>
		<iw:item id="dialog.title" alias="title" lang=""
				 readOnly="true" value="MODIFICATIONS NOT SAVED">
		</iw:item>
		<iw:item id="dialog.desc" alias="title" lang=""
				 readOnly="true" value="Warning! You have modified widget profiles, but did not save it.">
		</iw:item>
		<iw:item id="dialog.advise" alias="title" lang=""
				 readOnly="true" value="Do you want to save changes before leaving this page?">
		</iw:item>
		<iw:item id="dialog.btn.save" alias="discard" lang=""
				 readOnly="true" value="Save Changes">
		</iw:item>
		<iw:item id="dialog.btn.discard" alias="discard" lang=""
				 readOnly="true" value="Discard Changes">
		</iw:item>
		<iw:item id="dialog.btn.remain" alias="remain" lang=""
				 readOnly="true" value="Cancel">
		</iw:item>
	</iw:itemSet>
	<iw:itemSet id="widget.menu.labels">
		<iw:item id="mode.edit" alias="edit" lang=""
				 readOnly="true" value="Edit">
			<iw:alt lang="CNS" value="编辑"/>
		</iw:item>
		<iw:item id="mode.help" alias="help" lang=""
				 readOnly="true" value="Help">
			<iw:alt lang="CNS" value="帮助"/>
		</iw:item>
		<iw:item id="mode.config" alias="config" lang=""
				 readOnly="true" value="Preferences">
			<iw:alt lang="CNS" value="设置"/>
		</iw:item>
		<iw:item id="mode.refresh" alias="refresh" lang=""
				 readOnly="true" value="Refresh">
			<iw:alt lang="CNS" value="刷新"/>
		</iw:item>
		<iw:item id="win.state.maximize" alias="maximize" lang=""
				 readOnly="true" value="Maximize">
			<iw:alt lang="CNS" value="最大化"/>
		</iw:item>
		<iw:item id="win.state.minimize" alias="minimize" lang=""
				 readOnly="true" value="Minimize">
			<iw:alt lang="CNS" value="最小化"/>
		</iw:item>
		<iw:item id="win.state.restore" alias="restore" lang=""
				 readOnly="true" value="Restore">
			<iw:alt lang="CNS" value="还原"/>
		</iw:item>
		<iw:item id="win.state.expand" alias="expand" lang=""
				 readOnly="true" value="Expand">
			<iw:alt lang="CNS" value="扩展"/>
		</iw:item>
		<iw:item id="win.state.close" alias="close" lang=""
				 readOnly="true" value="Remove">
			<iw:alt lang="CNS" value="删除"/>
		</iw:item>
		<iw:item id="action.share" alias="share" lang=""
				readOnly="true" value="Share">
		</iw:item>
	</iw:itemSet>
	<iw:itemSet id="common.config.labels">
		<iw:item id="title" alias="title" lang=""
				 readOnly="true" value="Title:">
		</iw:item>
		<iw:item id="desc" alias="desc" lang=""
				 readOnly="true" value="Description:">
		</iw:item>
	</iw:itemSet>
	<iw:itemSet id="common.button">
		<iw:item id="button.done" alias="button" lang=""
				 readOnly="true" value="Done">
		</iw:item>
		<iw:item id="button.save" alias="button" lang=""
				 readOnly="true" value="Save">
		</iw:item>
		<iw:item id="button.cancel" alias="button" lang=""
				 readOnly="true" value="Cancel">
		</iw:item>
		<iw:item id="button.revert" alias="button" lang=""
				 readOnly="true" value="Revert">
		</iw:item>
	</iw:itemSet>
	<iw:content mode="wrapper"><![CDATA[
	<div class="component-control" id="_IWID_">
		<div class="widget-control">
			<div class="widget-id" style="display:none">_IWID_</div>
			<div class="decoration-titlebar">
				<div id="title__IWID_" class="decoration-title">
					<div class="title-icon"></div>
					<div class="title-text"></div>
				</div>
				<div class="widget-actions">
					<div class="cs-icon cs-icon-menu cs-mode-menu">operations</div>
					<div id="menu__IWID_" class="decoration-contextMenu" style="display: none"></div>
				</div>
				<div class="decoration-msgbar">
					<table><tr><td align="center">
						<div class="notice" style="display: none;">
							<div class="cs-icon"></div>
							<div class="message"></div>
						</div>
					</td></tr></table>
				</div>
			</div>
			<div class="csWidgetBody"/>
			<div class="decoration-toolbar"></div>
		</div> 
	</div>
	<script lang="javascript">$.CSWidget.includeStylesheet("widgets/wrappers/wrapper.css");</script>
	]]></iw:content>
	<iw:content mode="config" ><![CDATA[
	<div class="config-common">
		<div class="config-fields">
			<div class="config-field">
				<span class="title-label label"></span>
				<input size="30" class="field-value" name="title"/>
			</div>
			<div class="config-field">
				<span class="desc-label label"></span>
				<input size="30" class="field-value" name="description"/>
			</div>
		</div>
	</div>
	<script lang="javascript">$.CSWidget.includeStylesheet("widgets/modes/config.css");</script>
	]]></iw:content>
</iw:iwidget>